@model DAL_Data_Access_Layer_.CustomeModel.adminDashData;

<style>
    .iti {
        width: 100%;
    }

    .iti__country-list {
        z-index: 3 !important;
    }
</style><div class="container">    <div class=" mt-2 d-flex justify-content-between align-items-center">        <h4>Admin Profile</h4>        <a onclick="GetUserAccess(0)" class="btn shadow  btn-outline-info px-3 ">            <i class="fa-solid fa-angle-left fs-6"></i> Back        </a>    </div>    <div class="shadow border rounded my-3">        <div class="mx-3 my-4 ">            <form id="CreateAdminForm" onsubmit="adminAccCreate()">                <div class="mt-3">                    <h4 class="my-4 pt-4">Account Information</h4>                    <div class="row">                        <div class="mb-3 col-xl-4 col-lg-4 col-md-6 col-sm-12 form-floating  ">
                            <input type="text" class="form-control dark-mode2" asp-for="@Model._providerEdit.username" id="floatingInputUsername" placeholder="User Name" aria-label="User Name">                            <label class="ms-2 " for="floatingInputUsername">User Name</label>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.username"></span>                        </div>                        <div style="position: relative;" class="mb-3 col-xl-4 col-lg-4 col-md-6 col-sm-12 form-floating ">
                            <input asp-for="@Model._providerEdit.password" type="password" class="form-control inputs dark-mode2" placeholder="Password" id="adminCreatePassword" />                            <label for="exampleInputPassword1" class=" ms-2 innertext small">Password</label>                            <button class="btn show-password" style="right: 15px;position: absolute;top: 18%;" type="button">                                <i class="fa-regular fa-eye" onclick="adminCreatePasswordVisible2()" id="adminCreatePasswordBtn"></i>                            </button>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.password"></span>                            @* <span asp-validation-for="@Model._myProfile.password" class="text-danger"></span> *@                        </div>                        <div class="col-xl-4 col-lg-4 col-md-6 col-sm-12  form-floating mb-3 ">
                            <select asp-for="@Model._providerEdit.Roleid" class="form-select form-control dark-mode2" id="adminCreateRole">
                                @foreach (var obj in Model._role)
                                {
                                    <option value="@obj.Roleid">
                                        @obj.Name
                                    </option>
                                }
                            </select>

                            <label for="Role" class="ms-2 ">Role</label>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.Roleid"></span>                        </div>                       @*  <input  id="hidden1" hidden /> *@                    </div>                </div>                <div class="pt-4">                    <h4 class="mb-4">Administrator Information</h4>                    <div class="row mb-3">                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3 ">
                            <input asp-for="@Model._providerEdit.Firstname" type="text" class="d1class form-control dark-mode2" id="floatingInputFirstName" placeholder="First Name" aria-label="First Name">                            <label class="ms-2 " for="floatingInputFirstName">First Name</label>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.Firstname"></span>                        </div>                        <input  hidden />                        <input  hidden />                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12  form-floating mb-3 ">
                            <input asp-for="@Model._providerEdit.Lastname" type="text" class="d1class form-control dark-mode2" id="floatingInputLast" placeholder="Last Name" aria-label="Last Name">                            <label class="ms-2  " for="floatingInputLast">Last Name</label>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.Lastname"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input asp-for="@Model._providerEdit.Email" type="email" class="d1class form-control dark-mode2" id="ProfileInputemail" placeholder="Email" aria-label="Email">                            @* @ <span class="text-danger" asp-validation-for="@Model.AdminProfile.email"></span> @ *@                            <label class="ms-2 " for="floatingInputemail">Email</label>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.Email"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input asp-for="@Model._providerEdit.Con_Email" type="email" class="d1class form-control dark-mode2" id="ProfileInputemail2" placeholder="ConfirmEmail" aria-label="ConfirmEmail">                            @* @ <span class="text-danger" asp-validation-for="@Model.AdminProfile.confirmEmail"></span> @ *@                            <label class="ms-2 " for="floatingInputConfirmEmail">Confirm Email</label>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.Con_Email"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input asp-for="@Model._providerEdit.PhoneNumber" style="height:58px;" id="adminCreatePhoneNumber" class="form-control contact d2class dark-mode2" aria-label="Contact" type="number" />
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.PhoneNumber"></span>                        </div>                        <div class="d-flex col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <button class="btn border d-flex align-items-center w-100 regionbtn d1class dark-mode2" type="button" style="height:58px" data-bs-toggle="dropdown" aria-expanded="false">                                Select Region                            </button>                            <div class="dropdown-menu d1class">
                                @if (Model._RegionTable is not null && Model._RegionTable.Count > 0)                                {
                                    @foreach (var region in Model._RegionTable)                                    {                                        <div class="dropdown-item">                                            <input type="checkbox" name="regions" id="@region.Regionid" value="@region.Regionid" class="region-checkbox me-1 dark-mode2">                                            <label for="@region.Regionid">@region.Name</label>                                        </div>                                    }                                }                            </div>                        </div>                    </div>                </div>                <div class="pt-4">                    <h4 class="mb-4">Mailing And Billing Information</h4>                    <div class="row mb-3">                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input asp-for="@Model._providerEdit.Address1" type="text" class="dclass form-control dark-mode2" id="floatingInputAddress1" placeholder="Address1" aria-label="Address1">                            @* @ <span class="text-danger" asp-validation-for="@Model.AdminProfile.Address1"></span> @ *@                            <label class="ms-2 " for="floatingInputAddress1">Address1</label>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.Address1"></span>                        </div>                        <input  id="hidden3" hidden />                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input asp-for="@Model._providerEdit.Address2" type="text" class="dclass form-control dark-mode2" id="floatingInputAddress2" placeholder="Address2" aria-label="Address2">                            <label class="ms-2 " for="floatingInputAddress2">Address2</label>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.Address2"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input asp-for="@Model._providerEdit.city" type="text" class="dclass form-control dark-mode2" id="floatingInputCity" placeholder="City" aria-label="City">                            @* @ <span class="text-danger" asp-validation-for="@Model.AdminProfile.city"></span> @ *@                            <label class="ms-2 " for="floatingInputCity">City</label>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.city"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <select asp-for="@Model._providerEdit.Regionid" class="dclass form-select regionDropdown dark-mode2" id="state" aria-label="Region">
                                @foreach (var obj in Model._RegionTable.ToList())                                {                                    <option value="@obj.Regionid">                                        @obj.Name                                    </option>                                }                            </select>                            <label for="state" class="ms-2 ">State</label>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.Regionid"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input asp-for="@Model._providerEdit.zipcode" type="text" class="dclass form-control dark-mode2" id="floatingInputZipCode" placeholder="Zip Code" aria-label="Zip Code">                            @* @ <span class="text-danger" asp-validation-for="@Model.AdminProfile.zipcode"></span> @ *@                            <label class="ms-2" for="floatingInputZipCode">Zip</label>
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.zipcode"></span>                        </div>                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input asp-for="@Model._providerEdit.altPhone" style="height:58px;" id="adminCreateMobileTwo" class="form-control contact d3class dark-mode2" aria-label="Contact" type="number" />
                            <span class="text-danger" asp-validation-for="@Model._providerEdit.altPhone"></span>                        </div>                    </div>                </div>                <div class="d-grid gap-2 d-flex justify-content-center justify-content-md-end mb-3">                    <button type="submit" id="savebtn" class="btn btn-success text-white me-1">Create Account</button>                </div>            </form>        </div>    </div></div>

<partial name="_ValidationScriptsPartial" /><script>    var providerEditMbl = document.querySelector("#adminCreatePhoneNumber");    var providerEditMblMain = window.intlTelInput(providerEditMbl, {        utilsScript:            "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",    });    var providerEditMblTwo = document.querySelector("#adminCreateMobileTwo");    var providerEditMblTwoMain = window.intlTelInput(providerEditMblTwo, {        utilsScript:            "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",    });    function adminCreatePasswordVisible2() {        let a = document.getElementById("adminCreatePasswordBtn")        console.log(a)        if (a.classList.contains("fa-eye")) {            a.classList.remove("fa-eye")            a.classList.add("fa-eye-slash")        } else {            a.classList.remove("fa-eye-slash")            a.classList.add("fa-eye")        }        var x = document.getElementById("adminCreatePassword");        if (x.type === "password") {            x.type = "text";            console.log("hi1")        } else {            x.type = "password";            console.log("hi2")        }    }

    function adminAccCreate() {
        event.preventDefault();
        if ($("#CreateAdminForm").valid()) {            $.ajax({
                type: 'POST',
                url: "/adminDashboard/createAdmin",
                data: $("#CreateAdminForm").serialize(),

                success: function (result) {
                    createAdmin();
                    toastr.success("Account Created Successfully");
                },
                error: function () {
                    alert("Error");
                },
            });
        }
    }</script>