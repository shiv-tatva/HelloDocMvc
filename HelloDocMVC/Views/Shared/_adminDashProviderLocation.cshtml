<link rel="stylesheet" href="https://unpkg.com/leaflet/dist/leaflet.css" /><script src="https://cdn.jsdelivr.net/npm/leaflet.control.darkmode@1.0.0/dist/leaflet.control.darkmode.min.js"></script>@*<partial name="_AdminHeader" /> *@<div class="container d-flex flex-column justify-content-center align-content-center flex-column mb-5" style="height: 625px;">    <div class="req_form_container w-lg-50 d-flex justify-content-between mb-4">        <h3 class="fw-bold m-0 d-flex align-items-center">Provider Location</h3>        <button id="back-btn" onclick="location.href='@Url.Action("AdminDashboard", "Admin")'" hidden>&lt; Back</button>    </div>    <div id="map" class="w-100 h-100 shadow " style="z-index: 1;"></div></div><script>    var map = L.map('map').setView([22.2587, 71.1924], 8);    googleStreets = L.tileLayer('http://{s}.google.com/vt/lyrs=m&x={x}&y={y}&z={z}', {        maxZoom: 20,        subdomains: ['mt0', 'mt1', 'mt2', 'mt3']    }).addTo(map);    $.ajax({        url: '/adminDashboard/GetProviderLocation',        method: 'GET',        success: function (response) {            console.log(response);            response.map(function (res) {                L.marker([res.latitude, res.longitude]).addTo(map)                    .bindPopup(res.physicianname)            })        }    })</script>