@model DAL_Data_Access_Layer_.CustomeModel.partnerModel;<style>    .dataTables_info, .dataTables_length, .dataTables_paginate {        display: none !important;    }</style><link rel="stylesheet" href="https://cdn.datatables.net/1.13.7/css/dataTables.bootstrap5.min.css" /><div class="container-fluid">    <h4 class="mb-3 p-3 p-md-2 ">Vendor(s)</h4>    <div class="border rounded shadow p-3">        <div class="d-flex justify-content-between align-items-center mb-3">            <div class="d-lg-flex gap-2">                <form role="search" class=" d-flex align-items-center border border-rounded rounded border-info mb-2">                    <i type="search" class="bi bi-search ms-2 text-black" style="position: absolute;"></i>                    <input class="form-control border-0 my-search-input ps-5" id="my-search-input2" type="search" placeholder="Search Vendors" aria-label="Search">                </form>                <form role="search" class="d-flex align-items-center border border-rounded rounded border-info mb-2">                    <i type="search" class="bi bi-search px-2 " style="position: absolute;"></i>                    <select class="form-select border-0 ProfessionsDropdown ps-5" aria-label="Default select example" onchange="Partners(this.value)">                        <option value="">All Professions</option>                        @foreach (var obj in Model.Professions.ToList())
                        {                            <option value="@obj.Healthprofessionalid">                                @obj.Professionname                            </option>                        }                    </select>                </form>            </div>            <div class=" mb-3" id="buttonProvider" style="height: 47px;">                <button type="button" class="btn btn-outline-info d-md-block d-none" onclick="AddBusiness(0)">                    <i type="search" class="bi bi-plus-lg"> Add Business</i>                </button>                <button type="button" class="btn border-info text-light d-md-none d-block px-2 py-0" onclick="AddBusiness(0)">                    <i class="bi bi-plus text-info fs-3"></i>                </button>            </div>        </div>        <div class="table-responsive d-lg-block d-none">            <table class="table PartnerTable">                <thead style="background-color: rgb(219, 219, 219);">                    <tr>                        <th style="background: transparent;">Profession</th>                        <th style="background: transparent;">Business Name</th>                        <th style="background: transparent;">Email</th>                        <th style="background: transparent;">FAX Number</th>                        <th style="background: transparent;">Phone Number</th>                        <th style="background: transparent;">Business Contact</th>                        <th style="background: transparent;">Action</th>                    </tr>                </thead>                <tbody>                    @foreach (var obj in Model.Partnersdata)
                    {                        <tr class="align-middle dark-mode2">                            <td>@obj.ProfessionName</td>                            <td>@obj.VendorName</td>                            <td>@obj.VendorEmail</td>                            <td>@obj.FaxNo</td>                            <td>@obj.PhoneNo</td>                            <td>@obj.Businesscontact</td>                            <td>                                <div id="actionBtnGroup" style="display: flex;">                                    <button type="button" class="btn btn-outline-dark border-info text-info editbtn " onclick="AddBusiness(@obj.VendorId)">                                        Edit                                    </button>                                    <button type="button" class="btn btn-outline-dark border-info text-info delbtn ms-1 " onclick="DelPartner(@obj.VendorId)">                                        Delete                                    </button>                                </div>                            </td>                        </tr>                    }                </tbody>            </table>        </div>        <div class="border rounded shadow d-block d-lg-none">            <div class="accordion container-fluid p-0" id="accordionPanelsStayOpenExample">                <table class="w-100 PartnerTable">                    <thead>                        <tr>                            <th class="d-none"></th>                            <th class="d-none"></th>                        </tr>                    </thead>                    @{                        var counter = 1;                    }                    <tbody>                        @foreach (var obj in Model.Partnersdata)
                        {                            <tr class="d-flex flex-column">                                <td>                                    <h2 class="accordion-header" id="flush-heading@(counter)"></h2>                                    <div class="accordion-button collapsed d-flex flex-column pb-0" data-bs-toggle="collapse" data-bs-target="#flush-collapse@(counter)" aria-expanded="false" aria-controls="flush-collapse@(counter)">                                        <div class="w-100 ">                                            <span class="fw-bolder">                                                @obj.VendorName, @obj.ProfessionName                                            </span>                                        </div>                                    </div>                                </td>                                <td class="p-0">                                    <div id="flush-collapse@(counter)" class="accordion-collapse collapse" aria-labelledby="flush-heading@(counter)" data-bs-parent="#accordionFlushExample">                                        <div class="accordion-body main-flex pt-0">                                            <div class="inner-flex pt-2 mobile-font">                                                <span class="fw-bolder">Email</span>: @obj.VendorEmail                                            </div>                                            <div class="inner-flex mobile-font">                                                <span class="fw-bolder">Fax</span>: @obj.FaxNo                                            </div>                                            <div class="inner-flex mobile-font">                                                <span class="fw-bolder">Phone Number:</span>: @obj.PhoneNo                                            </div>                                            <div class="inner-flex pb-2 mobile-font">                                                <span class="fw-bolder">Business Contact</span>: @obj.Businesscontact                                            </div>                                            <div class="inner-flex d-flex gap-2 justify-content-end mt-1">                                                <button type="button" class="btn btn-outline-dark border-info text-info" onclick="AddBusiness(@obj.VendorId)">                                                    Edit                                                </button>                                                <button type="button" class="btn btn-outline-dark border-info text-info" onclick="DeletePartner(@obj.VendorId)">                                                    Delete                                                </button>                                            </div>                                        </div>                                    </div>                                </td>                            </tr>                            counter++;
                        }                    </tbody>                </table>            </div>        </div>    </div></div><script>    function exportBtnNew() {        // Get the HTML content of the table        var htmlContent = $("#tableParentDiv").html();        console.log(htmlContent)        // Set the modified HTML content to the input value        $("input[name='GridHtml']").val(htmlContent);    }    $(document).ready(function () {        $('.PartnerTable').DataTable({            "initComplete": function (settings, json) {                $('#my-search-input2').val(settings.oPreviousSearch.sSearch);                $('#my-search-input2').on('keyup', function () {                    var searchValue = $(this).val();                    settings.oPreviousSearch.sSearch = searchValue;                    settings.oApi._fnReDraw(settings);                });            },            "lengthMenu": [[5, 10, -1], [5, 10, "All"]],            "pageLength": -1,    @* "order": [[3, "desc"]],            "columns": [{ "searchable": true }, { "searchable": false }, { "searchable": true }, { "searchable": false }, { "searchable": false }, { "searchable": false }, { "searchable": false }, { "searchable": false }, { "searchable": false }], *@            language: {            oPaginate: {                sNext: '<i class="bi bi-caret-right-fill text-info"></i>',                sPrevious: '<i class="bi bi-caret-left-fill text-info"></i>'            }        }            });    // $('.dataTables_length').hide();    $('.dataTables_filter').hide();        });    function Partners(professionid) {        $.ajax({            method: "GET",            url: "/adminDashboard/Partners",            data: { professionid: professionid },            success: function (result) {                $("#content").html(result);                if (professionid != 0) {                    $(".ProfessionsDropdown").val(professionid);                }            },            error: function () {                alert("Error loading partial view");            },        });    }    function AddBusiness(vendorID) {        $.ajax({            method: "GET",            url: "/adminDashboard/AddBusiness",            data: { vendorID: vendorID },            success: function (result) {                $("#content").html(result);            },            error: function () {                alert("Error loading partial view");            },        });    }            function DelPartner(vendorID) {        $.ajax({            method: "GET",            url: "/adminDashboard/DelPartner",            data: { vendorID: vendorID },            success: function (result) {                partners();            },            error: function () {                alert("Error loading partial view");            },        });    }    </script>